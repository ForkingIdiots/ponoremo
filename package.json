{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:docs": "turbo build --filter=docs",
    "build:web": "turbo build --filter=web",
    "dev": "turbo dev",
    "dev:docs": "turbo dev --filter=docs",
    "dev:web": "turbo dev --filter=web",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "node-modules:clean": "rm -rf **/*/node_modules node_modules && pnpm install"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@8.15.6",
  "name": "with-vite"
}